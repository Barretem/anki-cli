{
  "name": "@barretter/anki-cli",
  "version": "0.0.1",
  "description": "cli tool",
  "main": "index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "test:ci": "cross-env NODE_ENV=test jest --ci -i --coverage --silent",
    "test:dev": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "updateSnapshot": "cross-env NODE_ENV=test jest --updateSnapshot",
    "build": "pnpm run clean && pnpm run prod",
    "dev": "tsc -w",
    "prod": "tsc",
    "clean": "rimraf dist",
    "test:anki": "node ./bin/anki sync --dirPath=./__mocks__/docs/demo --rootDeckName=demo"
  },
  "files": [
    "bin",
    "__tests__",
    "scripts",
    "src",
    "dist",
    "index.js",
    "global.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Barretem/anki-cli.git"
  },
  "bin": {
    "anki": "bin/anki"
  },
  "keywords": [
    "anki"
  ],
  "engines": {
    "node": ">=16"
  },
  "author": "Barret",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^22.9.0",
    "axios": "^1.7.7",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0",
    "eslint": "^8.12.0",
    "marked": "4.2.12",
    "minimist": "^1.2.8",
    "node-html-markdown": "^1.3.0",
    "ora": "^5.4.1",
    "prismjs": "^1.29.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "babel-jest": "^29.5.0",
    "cross-env": "^7.0.3",
    "jest": "^29.3.1",
    "jest-cli": "^29.3.1",
    "jest-environment-node": "^29.5.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.7.4"
  }
}
